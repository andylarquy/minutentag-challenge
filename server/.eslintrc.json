{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["import", "prettier", "@typescript-eslint"],
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "semi": ["warn", "never"],
    "eol-last": ["warn", "always"]
  }
}
